<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset name="fields">

              <field name="id" type="hidden" default="0" label="JGLOBAL_FIELD_ID_LABEL"
                    readonly="true" class="readonly"
                    description="JGLOBAL_FIELD_ID_DESC" /> 

       
					<field type="wbtyfile" label="COM_WBTY_AUDIO_MANAGER_FORM_LBL_AUDIO_FILES_FILE_NAME" name="file_name" description="COM_WBTY_AUDIO_MANAGER_FORM_DESC_AUDIO_FILES_FILE_NAME" directory="audio_files" class="default_col" />
					<field type="text" label="COM_WBTY_AUDIO_MANAGER_FORM_LBL_AUDIO_FILES_TITLE" name="title" filter="safehtml" required="true" />
					<field type="textarea" label="COM_WBTY_AUDIO_MANAGER_FORM_LBL_AUDIO_FILES_DESCRIPTION" name="description" filter="safehtml" />
					<field type="checkbox" label="Include in List" name="include_in_list" filter="safehtml" value="1" />

         <field
                name="state"
                type="hidden"
                label="JSTATUS"
                description="JFIELD_PUBLISHED_DESC"
                class="inputbox"
                size="1"
                default="1">
                <option value="1">JPUBLISHED</option>
                <option value="0">JUNPUBLISHED</option>
            </field> 

                <field name="checked_out" type="hidden" filter="unset" />
                <field name="checked_out_time" type="hidden" filter="unset" />
				
				<field name="created_by" type="hidden" filter="integer" />
				<field name="created_time" type="hidden" filter="safehtml" />
				
				<field name="modified_by" type="hidden" filter="integer" overwrite="true" />
				<field name="modified_time" type="hidden" filter="safehtml" overwrite="true" />

	</fieldset>

</form>
